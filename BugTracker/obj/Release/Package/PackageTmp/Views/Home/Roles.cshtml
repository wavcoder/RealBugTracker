
<h2> Manage RoleUsers</h2>



<h2>Manage </h2>  
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        <div class="form-group">
            <h2>Role</h2>
            <div class="col-md-10">
                @Html.DropDownList("Users Not In Role", null, htmlAttributes: new { @class = "form-control" })
            </div>
        </div>


        <div class="form-group">
            <h2>User</h2>
            <div class="col-md-10">
                @Html.DropDownList("Users Currently In Role", null, htmlAttributes: new { @class = "form-control" })
            </div>
        </div>
    </div>
}