    <div class="form-horizontal">

        <div class="form-group">
           <h2>Role</h2>
            <div class="col-md-10">
                @Html.DropDownList("UserList", null, htmlAttributes: new { @class = "form-control" })
            </div>
        </div>
 

        <div class="form-group">
            <h2>User</h2>
            <div class="col-md-10">
                @Html.DropDownList("RolesList", null, htmlAttributes: new { @class = "form-control" })
            </div>
        </div>
     </div>
}